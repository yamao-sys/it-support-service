{
  "name": "it-support",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "prepare": "cd ../.. && husky frontend/it-support/.husky",
    "lint-staged": "lint-staged",
    "test:e2e": "turbo run test:e2e",
    "test:e2e:registration": "turbo test:e2e --filter=registration",
    "test:e2e:business": "turbo test:e2e --filter=business",
    "gen:schema": "turbo run gen:schema && pnpm format",
    "gen:api-spec": "turbo run gen:api-spec",
    "gen:api-spec:watch": "turbo run gen:api-spec:watch",
    "diff:api-spec:check": "turbo run diff:api-spec:check"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^22.13.4",
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/openapi": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest",
    "autoprefixer": "^10.4.20",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.20.0",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.4.3",
    "openapi-typescript": "^7.6.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/openapi": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "openapi-fetch": "^0.13.4",
    "react-datepicker": "^8.2.1",
    "react-hook-form": "^7.54.2"
  }
}
