{
  "name": "registration",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "check-types": "tsc --noEmit",
    "dev:test": "NODE_ENV=test next dev --turbopack --port 3100",
    "test:e2e": "playwright test",
    "gen:schema": "pnpx tsx /app/it-support/generate_api_types.ts /app/it-support/openapi/registration/reference.yaml /app/it-support/apps/registration/apis/generated/",
    "gen:api-spec": "cd api-spec && npx tsp compile . && node patch.mjs && cd ..",
    "gen:api-spec:watch": "cd api-spec && npx tsp compile . --watch && cd .."
  },
  "dependencies": {
    "@repo/ui": "workspace:*",
    "next": "^15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/playwright-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/api-spec-config": "workspace:*",
    "@types/node": "^22",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "typescript": "5.7.3"
  }
}
